{
	"manifest_version":2,
	
	"name":"Google_translate",
	"description":"Translate toolkit",
	"background":{
		"scripts":["background.js"]
	},
	"browser_action" : {
		"default_icon" : "icon.png",
		"default_popup" : "index.html"
	},
	"version":"1.2",
	
	"icons":{
		"64":"icon.png",
		"32":"icon.png",
		"16":"icon.png",
		"128":"icon.png"
	},
	"permissions":[
		"contextMenus",
		"notifications"


	]

}